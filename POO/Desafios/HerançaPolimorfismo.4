package Desafio5;

public class ClienteEspecial extends Conta_Corrente{

    public double desconto = 0.001;

    public ClienteEspecial(double _saque, double _deposito, double _saldo) {
        super(_saque, _deposito, _saldo);
    }
    public double valorDebitado(double valorsaque) {
        return (valorsaque * desconto);
    }
}
